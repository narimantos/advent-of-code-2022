   "source": [
    "import pandas as pd\n",
    "import numpy as np\n",
    "\n",
    "with open(\"input.txt\") as f:\n",
    "   Elfs = f.read()\n",
    "\n",
    "Elfs = Elfs.split(\"\\n\\n\")\n",
    "Elfs = [lines.split('\\n') for lines in Elfs]\n",
    "\n",
    "df = pd.DataFrame(Elfs).fillna(value=np.nan).replace(np.nan,0).apply(pd.to_numeric)\n",
    "\n",
    "df = df.sum(1).max()\n",
    "print(df)\n",
    "\n",
    "\n"
   ]
  }
